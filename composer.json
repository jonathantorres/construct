{
    "name": "jonathantorres/construct",
    "description": "Cli command to generate a basic PHP project structure.",
    "keywords": ["php", "project", "structure", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Torres",
            "email": "jonathantorres41@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "2.6.*",
        "illuminate/filesystem": "5.0.*",
        "illuminate/support": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "JonathanTorres\\Construct\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JonathanTorres\\Construct\\Tests\\": "tests/"
        }
    },
    "bin": ["construct"],
    "minimum-stability": "stable"
}
